<script>
    const navigations = [ ];

  {% for block in section.blocks %}
    {% if block.type == 'navigation' %}
      {% for link in linklists[block.settings.menu].links %}
        navigations.push({
          link: "{{ link.url }}",
          label: "{{ link.title }}",
        });
      {% endfor %}
    {% endif %}
  {% endfor %}

    let supplyChainImgLoaded = [];

    const navigateContainer = document.getElementById('service-cable-management-head-top-navigate');

    if (!navigateContainer) {
    console.error("Error: Navigation container not found!");
    } else {
    navigations.forEach(item => {
    const a = document.createElement('a');
    a.href = item.link; // Directly assign the link
    a.className = 'navigate-link';

    const img = document.createElement('img');
    img.src = "{{ 'icon-curved-arrow.svg' | asset_url }}"; // Ensure correct asset path
    img.alt = "Navigation Arrow";
    img.width = 24;
    img.height = 24;

    const p = document.createElement('p');
    p.textContent = item.label;

    a.appendChild(img);
    a.appendChild(p);

    a.addEventListener('click', function (e) {
    // If the link is for scrolling within the same page (anchors)
    if (item.link.startsWith('#')) {
    e.preventDefault();

    const targetId = item.link.substring(1); // Remove #
    const targetElement = document.getElementById(targetId);

    if (targetElement) {
    const rect = targetElement.getBoundingClientRect();
    const scrollPosition = rect.top + window.pageYOffset - 100; // Adjust for headers

    window.scrollTo({
    top: scrollPosition,
    behavior: 'smooth',
    });
    } else {
    console.warn("Target element not found:", targetId);
    }
    } else {
    // If it's an external link, navigate normally
    window.location.href = item.link;
    }
    });

    navigateContainer.appendChild(a);
    });
    }
    function handleImageLoad(imgId, skeletonId) {
      const skeleton = document.getElementById(skeletonId);
      const img = document.getElementById(imgId);

      if (skeleton && img) {
        skeleton.style.setProperty("display", "none", "important");
        img.style.display = "block";
      }
    }
    function handleSupplyChainImageLoad(imgId) {
      if (!supplyChainImgLoaded.includes(imgId)) supplyChainImgLoaded.push(imgId);
      if (supplyChainImgLoaded.length === 3) {
        const skeletons = document.querySelectorAll(".skeleton-loader.supply-skeleton");
        const imgs = document.querySelectorAll(".service-cable-supply-image");
        skeletons.forEach(skeleton => {
          skeleton.style.setProperty("display", "none", "important");
        });
        imgs.forEach(img => {
          img.style.display = "block";
        })
      }
    }
</script>
